  archiveprefix = {arxiv},
  keywords = {High Energy Physics - Phenomenology,Nuclear Theory},
  file = {/home/matonoliv/snap/zotero-snap/common/Zotero/storage/ZRKPQ8JA/Wang - 1998 - Effect of Jet Quenching on High-$p_T$ Hadron Spect.pdf;/home/matonoliv/snap/zotero-snap/common/Zotero/storage/YFUVBULU/9804357.html},
  title = {Effect of {{Jet Quenching}} on {{High-}}{$p_T$} {{Hadron Spectra}} in {{High-energy Nuclear Collisions}}}
