  publisher = {{arXiv}},
  doi = {10.48550/arXiv.1512.07227},
  urldate = {2023-04-11},
  abstract = {The multi-strange baryon yields in Pb--Pb collisions have been shown to exhibit an enhancement relative to pp reactions. In this work, \$\textbackslash Xi\$ and \$\textbackslash Omega\$ production rates have been measured with the ALICE experiment as a function of transverse momentum, \$\{p\_\{\textbackslash rm T\}\}\$, in p-Pb collisions at a centre-of-mass energy of \$\{\textbackslash sqrt\{s\_\{\textbackslash rm NN\}\}\}\$ = 5.02 TeV. The results cover the kinematic ranges 0.6 GeV/\$c{$<\lbrace$}p\_\{\textbackslash rm T\}\vphantom\{\} {$<\$$}7.2 GeV/\$c\$ and 0.8 GeV/\$c{$<\lbrace$}p\_\{\textbackslash rm T\}\vphantom\{\}{$<\$$} 5 GeV/\$c\$, for \$\textbackslash Xi\$ and \$\textbackslash Omega\$ respectively, in the common rapidity interval -0.5 \${$<\lbrace$}y\_\{\textbackslash rm CMS\}\vphantom\{\}{$<\$$} 0. Multi-strange baryons have been identified by reconstructing their weak decays into charged particles. The \$\{p\_\{\textbackslash rm T\}\}\$ spectra are analysed as a function of event charged-particle multiplicity, which in p-Pb collisions ranges over one order of magnitude and lies between those observed in pp and Pb-Pb collisions. The measured \$\{p\_\{\textbackslash rm T\}\}\$ distributions are compared to the expectations from a Blast-Wave model. The parameters which describe the production of lighter hadron species also describe the hyperon spectra in high multiplicity p-Pb. The yield of hyperons relative to charged pions is studied and compared with results from pp and Pb-Pb collisions. A statistical model is employed, which describes the change in the ratios with volume using a canonical suppression mechanism, in which the small volume causes a species-dependent relative reduction of hadron production. The calculations, in which the magnitude of the effect depends on the strangeness content, show good qualitative agreement with the data.},
  archiveprefix = {arxiv},
  keywords = {High Energy Physics - Experiment,Nuclear Experiment},
  file = {/home/matonoliv/snap/zotero-snap/common/Zotero/storage/DB7SUP8B/ALICE Collaboration - 2016 - Multi-strange baryon production in p-Pb collisions.pdf;/home/matonoliv/snap/zotero-snap/common/Zotero/storage/CQSYQ5E5/1512.html},
  title = {Multi-Strange Baryon Production in p-{{Pb}} Collisions at {$\sqrt{s_\mathbf{NN}}=5.02$} {{TeV}}}
}

