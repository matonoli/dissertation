  author = {Collaboration, The CMS and {d'Enterria}, D. and Ballintijn, M. and Bedjidian, M. and Hofman, D. and Kodolova, O. and Loizides, C. and Lokthin, I. P. and Louren{\c c}o, C. and Mironov, C. and Petrushanko, S. V. and Roland, C. and Roland, G. and Sikler, F. and Veres (editors), G.},
