@article{behrendsInclusiveHadronProduction1985,
  title = {Inclusive {{Hadron Production}} in {{Upsilon Decays}} and in {{Nonresonant}} Electron-{{Positron Annihilation}} at 10.49-{{GeV}}},
  author = {Behrends, S. and Chadwick, K. and Gentile, T. and Guida, Jan M. and Guida, Joan A. and Melissinos, A. C. and Olsen, S. L. and Parkhurst, G. and Poling, R. and Rosenfeld, C.},
