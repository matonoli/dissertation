@article{schmidtProtectionCERNLarge2006,
  title = {Protection of the {{CERN Large Hadron Collider}}},
  author = {Schmidt, R. and Assmann, R. and Carlier, E. and Dehning, B. and Denz, R. and Goddard, B. and Holzer, E. B. and Kain, V. and Puccio, B. and Todd, B. and Uythoven, J. and Wenninger, J. and Zerlauth, M.},
