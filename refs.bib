@article{theatlascollaborationDetailedMapHiggs2022,
  title = {A Detailed Map of {{Higgs}} Boson Interactions by the {{ATLAS}} Experiment Ten Years after the Discovery},
  author = {{The ATLAS Collaboration}},
  year = {2022},
  month = jul,
  journal = {Nature},
  volume = {607},
  number = {7917},
  pages = {52--59},
  publisher = {{Nature Publishing Group}},
  issn = {1476-4687},
  doi = {10.1038/s41586-022-04893-w},
  urldate = {2022-09-21},
  abstract = {The standard model of particle physics1\textendash 4 describes the known fundamental particles and forces that make up our Universe, with the exception of gravity. One of the central features of the standard model is a field that permeates all of space and interacts with fundamental particles5\textendash 9. The quantum excitation of this field, known as the Higgs field, manifests itself as the Higgs boson, the only fundamental particle with no spin. In 2012, a particle with properties consistent with the Higgs boson of the standard model~was observed by the ATLAS and CMS experiments at the Large Hadron Collider at CERN10,11. Since then, more than 30\,times as many Higgs bosons have been recorded by the ATLAS experiment, enabling much more precise measurements and new tests of the theory. Here, on the basis of this larger dataset, we combine an unprecedented number of production and decay processes of the Higgs boson to scrutinize its interactions with elementary particles. Interactions with gluons, photons, and W and Z bosons\textemdash the carriers of the strong, electromagnetic and weak forces\textemdash are studied in detail. Interactions with three third-generation matter particles (bottom (b) and top (t) quarks, and~tau leptons ({$\tau$})) are well measured and indications of interactions with a second-generation particle (muons, {$\mu$}) are emerging. These tests reveal that the Higgs boson discovered ten years ago is remarkably consistent with the predictions of the theory and provide stringent constraints on many models of new phenomena beyond the standard model.},
  copyright = {2022 The Author(s)},
  langid = {english},
  keywords = {Experimental particle physics,Particle physics},
  file = {/home/matonoliv/snap/zotero-snap/common/Zotero/storage/8B2U8GTD/The ATLAS Collaboration - 2022 - A detailed map of Higgs boson interactions by the .pdf;/home/matonoliv/snap/zotero-snap/common/Zotero/storage/NQHRH88K/s41586-022-04893-w.html}
}

@article{theatlascollaborationObservationNewParticle2012,
  title = {Observation of a New Particle in the Search for the {{Standard Model Higgs}} Boson with the {{ATLAS}} Detector at the {{LHC}}},
  author = {{The ATLAS Collaboration}},
  year = {2012},
  month = sep,
  journal = {Physics Letters B},
