@article{alicecollaborationProductionChargedPions2020,
  author = {ALICE Collaboration},
  year = {2020},
  month = apr,
  journal = {Phys. Rev. C},
  volume = {101},
  number = {4},
  eprint = {1910.07678},
  primaryclass = {hep-ex, physics:nucl-ex},
  pages = {044907},
  issn = {2469-9985, 2469-9993},
  doi = {10.1103/PhysRevC.101.044907},
  urldate = {2022-09-13},
  abstract = {Mid-rapidity production of \$\textbackslash pi\^\{\textbackslash pm\}\$, \$\textbackslash rm\{K\}\^\{\textbackslash pm\}\$ and (\$\textbackslash bar\{\textbackslash rm\{p\}\}\$)p measured by the ALICE experiment at the LHC, in Pb-Pb and inelastic pp collisions at \$\textbackslash sqrt\{s\_\{\textbackslash rm\{NN\}\}\}\$ = 5.02 TeV, is presented. The invariant yields are measured over a wide transverse momentum (\$p\_\{\textbackslash rm\{T\}\}\$) range from hundreds of MeV/\$c\$ up to 20 GeV/\$c\$. The results in Pb-Pb collisions are presented as a function of the collision centrality, in the range 0\$-\$90\%. The comparison of the \$p\_\{\textbackslash rm\{T\}\}\$-integrated particle ratios, i.e. proton-to-pion (p/\$\textbackslash pi\$) and kaon-to-pion (K/\$\textbackslash pi\$) ratios, with similar measurements in Pb-Pb collisions at \$\textbackslash sqrt\{s\_\{\textbackslash rm\{NN\}\}\}\$ = 2.76 TeV show no significant energy dependence. Blast-wave fits of the \$p\_\{\textbackslash rm\{T\}\}\$ spectra indicate that in the most central collisions radial flow is slightly larger at 5.02 TeV with respect to 2.76 TeV. Particle ratios (p/\$\textbackslash pi\$, K/\$\textbackslash pi\$) as a function of \$p\_\{\textbackslash rm\{T\}\}\$ show pronounced maxima at \$p\_\{\textbackslash rm\{T\}\}\$ \$\textbackslash approx\$ 3 GeV/\$c\$ in central Pb-Pb collisions. At high \$p\_\{\textbackslash rm\{T\}\}\$, particle ratios at 5.02 TeV are similar to those measured in pp collisions at the same energy and in Pb-Pb collisions at \$\textbackslash sqrt\{s\_\{\textbackslash rm\{NN\}\}\}\$ = 2.76 TeV. Using the pp reference spectra measured at the same collision energy of 5.02 TeV, the nuclear modification factors for the different particle species are derived. Within uncertainties, the nuclear modification factor is particle species independent for high \$p\_\{\textbackslash rm\{T\}\}\$ and compatible with measurements at \$\textbackslash sqrt\{s\_\{\textbackslash rm\{NN\}\}\}\$ = 2.76 TeV. The results are compared to state-of-the-art model calculations, which are found to describe the observed trends satisfactorily.},
  archiveprefix = {arxiv},
  keywords = {High Energy Physics - Experiment,Nuclear Experiment},
  file = {/home/matonoliv/snap/zotero-snap/common/Zotero/storage/G4S8SQJC/ALICE Collaboration - 2020 - Production of charged pions, kaons and (anti-)prot.pdf;/home/matonoliv/snap/zotero-snap/common/Zotero/storage/3NCWEH7Z/1910.html},
  title = {Production of Charged Pions, Kaons and (Anti-)Protons in {{Pb-Pb}} and Inelastic Pp Collisions at {$\sqrt{s_{\rm{NN}}}$} = 5.02 {{TeV}}}
}

@misc{alicecollaborationProductionPionsKaons2023,
