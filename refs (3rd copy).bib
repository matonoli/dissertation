@article{chenCentralityDependenceProductions2015,
  title = {Centrality {{Dependence}} of {{Productions}} for {{Single Hadrons}} and {{Inclusive Jets}} in {{High-Energy}} p + {{A Collisions}} with {{NLO QCD}}*},
  author = {Chen, Shi-Yong and Shen, Ke-Ming and Dai, Wei and Zhang, Ben-Wei and Zhang, Han-Zhong and Wang, En-Ke},
  year = {2015},
  month = jul,
  journal = {Commun. Theor. Phys.},
  volume = {64},
  number = {1},
  pages = {95},
  publisher = {{Chinese Physical Society and IOP Publishing}},
  issn = {0253-6102},
  doi = {10.1088/0253-6102/64/1/95},
  urldate = {2023-04-26},
  abstract = {By using the recent spatially dependent nuclear PDF set EPS09s, we investigated the centrality-dependent Cold Nuclear Matter (CNM) effects for neutral {$\pi$}, {$\eta$} mesons and inclusive jets at RHIC in d+Au collisions and at LHC in p+Pb collisions. The nuclear modification factors as functions of transverse momentum are plotted at different centralities bins respectively. At all fixed centralities, the nuclear modification factors show no significant suppressions, contrast to the strong suppressions observed for central Au+Au collisions. Our results are consistent with the PHENIX preliminary Data in minimum bias and central d+Au collisions. The LHC experimental Data also support our predictions for both single inclusive hadron and inclusive jets productions in central p+Pb collisions. And the centrality dependence of the nuclear suppressions for all the observations in our calculations are lower than the RHIC and LHC Data.},
  langid = {english}
}

