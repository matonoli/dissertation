@article{alicecollaborationInvestigationsAnisotropicFlow2019,
  title = {Investigations of Anisotropic Flow Using Multi-Particle Azimuthal Correlations in Pp, p-{{Pb}}, {{Xe-Xe}}, and {{Pb-Pb}} Collisions at the {{LHC}}},
  author = {ALICE Collaboration},
  year = {2019},
