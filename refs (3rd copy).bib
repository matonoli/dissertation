@article{ortizEnergyDependenceUnderlyingevent2021,
  title = {Energy Dependence of Underlying-Event Observables from {{RHIC}} to {{LHC}} Energies},
  author = {Ortiz, Antonio},
  year = {2021},
  month = oct,
  journal = {Phys. Rev. D},
  volume = {104},
  number = {7},
  eprint = {2108.08360},
  primaryclass = {hep-ph, physics:nucl-th},
  pages = {076019},
  issn = {2470-0010, 2470-0029},
  doi = {10.1103/PhysRevD.104.076019},
  urldate = {2023-04-23},
  abstract = {A study of the charged-particle density (number density) in the transverse region of the di-hadron correlations exploiting the existing pp and p\$\textbackslash bar\{\textbackslash rm p\}\$ data from RHIC to LHC energies is reported. This region has contributions from the Underlying Event (UE) as well as from Initial- and Final-State Radiation (ISR-FSR). Based on the data, a two-component model is built. This has the functional form \$\textbackslash propto s\^\{\textbackslash alpha\}+\textbackslash beta\textbackslash log(s)\$, where the logarithmic ((\$\textbackslash beta = 0.140 \textbackslash pm 0.007\$)) and the power-law (\$\textbackslash alpha = 0.270 \textbackslash pm 0.005\$) terms describe the components more sensitive to the ISR-FSR and UE contributions, respectively. The model describes the data from RHIC to LHC energies, the extrapolation to higher energies indicates that at around \$\textbackslash sqrt\{s\} \textbackslash approx 100\$\textbackslash,TeV the number density associated to UE will match that from ISR-FSR. Although this behaviour is not predicted by PYTHIA\textasciitilde 8.244, the power-law behaviour of the UE contribution is consistent with the energy dependence of the parameter that regulates Multiparton Interactions. Using simulations, KNO-like scaling properties of the multiplicity distributions in the regions sensitive to either UE or ISR-FSR are also discussed. The results presented here can be helpful to constrain QCD-inspired Monte Carlo models at the Future Circular Collider energies, as well as to characterize the UE-based event classifiers which are currently used at the LHC.},
  archiveprefix = {arxiv},
  keywords = {High Energy Physics - Phenomenology,Nuclear Theory},
  file = {/home/matonoliv/snap/zotero-snap/common/Zotero/storage/KHPH4SSU/Ortiz - 2021 - Energy dependence of underlying-event observables .pdf;/home/matonoliv/snap/zotero-snap/common/Zotero/storage/5MTRHC7L/2108.html}
}

