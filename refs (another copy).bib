  eprint = {0808.1854},
  primaryclass = {hep-ph},
  publisher = {{arXiv}},
  doi = {10.48550/arXiv.0808.1854},
  urldate = {2023-04-24},
  abstract = {The combined HERA-I data set, of neutral and charged current inclusive cross-sections for \$e\^+p\$ and \$e\^-p\$ scattering, is used as the sole input for a next-to-leading order (NLO) QCD parton distribution function (PDF) fit. The consistent treatment of systematic uncertainties in the joint data set ensures that experimental uncertainties on the PDFs can be calculated without need for an increased \$\textbackslash chi\^2\$ tolerance. This results in PDFs with greatly reduced experimental uncertainties compared to the separate analyses of the ZEUS and H1 experiments. Model uncertainties, including those arising from parametrization dependence, are also carefully considered. The resulting HERAPDFs have impressive precision compared to the global fits.},
  archiveprefix = {arxiv},
  keywords = {High Energy Physics - Phenomenology},
  file = {/home/matonoliv/snap/zotero-snap/common/Zotero/storage/3B6V3FDT/Cooper-Sarkar - 2008 - Extraction of the proton parton density functions .pdf;/home/matonoliv/snap/zotero-snap/common/Zotero/storage/6HYMQGWW/0808.html}
}

