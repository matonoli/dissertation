@article{alicecollaborationInvestigationsAnisotropicFlow2019,
  title = {Investigations of Anisotropic Flow Using Multi-Particle Azimuthal Correlations in Pp, p-{{Pb}}, {{Xe-Xe}}, and {{Pb-Pb}} Collisions at the {{LHC}}},
  author = {{ALICE Collaboration}},
  year = {2019},
  month = oct,
  journal = {Phys. Rev. Lett.},
  volume = {123},
  number = {14},
  eprint = {1903.01790},
  primaryclass = {hep-ex, physics:nucl-ex},
  pages = {142301},
  issn = {0031-9007, 1079-7114},
  doi = {10.1103/PhysRevLett.123.142301},
  urldate = {2023-04-09},
  abstract = {Measurements of anisotropic flow coefficients (\$v\_n\$) and their cross-correlations using two- and multi-particle cumulant methods are reported in collisions of pp at \$\textbackslash sqrt\{s\} = 13\$ TeV, p-Pb at \$\textbackslash sqrt\{s\_\{\_\{\textbackslash rm NN\}\}\} = 5.02\$ TeV, Xe-Xe at \$\textbackslash sqrt\{s\_\{\_\{\textbackslash rm NN\}\}\} = 5.44\$ TeV, and Pb-Pb at \$\textbackslash sqrt\{s\_\{\_\{\textbackslash rm NN\}\}\} = 5.02\$ TeV recorded with the ALICE detector. The multiplicity dependence of \$v\_n\$ is studied in a very wide range from 20 to 3000 particles produced in the mid-rapidity region \$|\textbackslash eta|{$<$}0.8\$ for the transverse momentum range \$0.2 {$<$} p\_\{\textbackslash rm T\} {$<$} 3.0\$ GeV/\$c\$. An ordering of the coefficients \$v\_2 {$>$} v\_3 {$>$} v\_4\$ is found in pp and p-Pb collisions, similar to that seen in large collision systems, while a weak \$v\_2\$ multiplicity dependence is observed relative to nucleus-nucleus collisions in the same multiplicity range. Using a novel subevent method, \$v\_\{2\}\$ measured with four-particle cumulants is found to be compatible with that from six-particle cumulants in pp and p-Pb collisions. The magnitude of the correlation between \$v\_n\^2\$ and \$v\_m\^2\$, evaluated with the symmetric cumulants SC\$(m,n)\$ is observed to be positive at all multiplicities for \$v\_2\$ and \$v\_4\$, while for \$v\_2\$ and \$v\_3\$ it is negative and changes sign for multiplicities below 100, which may indicate a different \$v\_n\$ fluctuation pattern in this multiplicity range. The observed long-range multi-particle azimuthal correlations in high multiplicity pp and p-Pb collisions can neither be described by PYTHIA 8 nor by IP-Glasma+MUSIC+UrQMD model calculations, and hence provide new insights into the understanding of collective effects in small collision systems.},
  archiveprefix = {arxiv},
  keywords = {High Energy Physics - Experiment,Nuclear Experiment},
  file = {/home/matonoliv/snap/zotero-snap/common/Zotero/storage/98SSJCSW/ALICE Collaboration - 2019 - Investigations of anisotropic flow using multi-par.pdf;/home/matonoliv/snap/zotero-snap/common/Zotero/storage/EITDEFEM/1903.html}
}

