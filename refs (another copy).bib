@article{bencediApparentModificationJetlike2020,
  title = {Apparent Modification of the Jet-like Yield in Proton\textendash Proton Collisions with Large Underlying Event},
  author = {Benc{\'e}di, Gyula and Ortiz, Antonio and Tripathy, Sushanta},
  year = {2020},
