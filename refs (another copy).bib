  file = {/home/matonoliv/snap/zotero-snap/common/Zotero/storage/LAKT2WL2/ALICE Collaboration - 2019 - Charged-particle production as a function of multi.pdf;/home/matonoliv/snap/zotero-snap/common/Zotero/storage/2UII8FST/1905.html},
  title = {Charged-Particle Production as a Function of Multiplicity and Transverse Spherocity in Pp Collisions at {$\sqrt{s}$} = 5.02 and 13 {{TeV}}}
