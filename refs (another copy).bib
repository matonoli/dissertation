  journal = {J. Phys. G: Nucl. Part. Phys.},
  volume = {48},
  number = {1},
  pages = {015007},
  publisher = {{IOP Publishing}},
  issn = {0954-3899},
  doi = {10.1088/1361-6471/abc5fb},
  urldate = {2023-04-04},
  abstract = {This paper presents the production of charged particles associated with high-p T trigger particles ( GeV/c) at midrapidity in proton\textendash proton collisions at TeV simulated with PYTHIA 8.244. The study is performed as a function of the relative transverse activity classifier, R T, which is the relative charged-particle multiplicity in the transverse region ({$\pi$}/3 {$<$} |{$\phi$} trig. - {$\phi$} assoc.| {$<$} 2{$\pi$}/3) of the di-hadron correlations, and it is sensitive to the multi-parton interactions. The evolution of the yield of associated particles on both the towards and the away regions ( GeV/c) as a function of R T is investigated. We propose a strategy which allows for the modelling and subtraction of the underlying event contribution from the towards and the away regions in challenging environments like those characterised by large R T. We found that the signal in the away region becomes broader with increasing R T, while its corresponding yield is independent of R T. Contrarily, the yield increases with R T in the towards region. This effect is reminiscent of that seen in heavy-ion collisions, where an enhancement of the yield in the towards region for 0\%\textendash 5\% central Pb\textendash Pb collisions at TeV was reported. A discussion on the origin of these effects in PYTHIA, as well as their implications for the interpretation of recent LHC results for pp collisions, is presented.},
  langid = {english},
  file = {/home/matonoliv/snap/zotero-snap/common/Zotero/storage/TZUEH6ME/Bencédi et al. - 2020 - Apparent modification of the jet-like yield in pro.pdf}
}

