  title = {Jet-like Correlations with Respect to {{K}}{$^{0}_{\rm S}$} and {$\Lambda$} ({$\bar{\Lambda}$}) in Pp and {{Pb-Pb}} Collisions at {$\mathbf{\it\sqrt{s_\mathrm{NN}}}$} = 5.02 {{TeV}}}
}

