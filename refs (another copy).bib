@article{cmscollaborationEvidenceCollectivityPp2017,
  title = {Evidence for Collectivity in Pp Collisions at the {{LHC}}},
  author = {{CMS Collaboration}},
  year = {2017},
  month = feb,
  journal = {Physics Letters B},
  volume = {765},
  eprint = {1606.06198},
  primaryclass = {hep-ex, physics:nucl-ex},
  pages = {193--220},
  issn = {03702693},
  doi = {10.1016/j.physletb.2016.12.009},
  urldate = {2023-04-09},
  abstract = {Measurements of two- and multi-particle angular correlations in pp collisions at sqrt(s) = 5, 7, and 13 TeV are presented as a function of charged-particle multiplicity. The data, corresponding to integrated luminosities of 1.0 inverse picobarn (5 TeV), 6.2 inverse picobarns (7 TeV), and 0.7 inverse picobarns (13 TeV), were collected using the CMS detector at the LHC. The second-order (v[2]) and third-order (v[3]) azimuthal anisotropy harmonics of unidentified charged particles, as well as v[2] of K0 short and Lambda/anti-Lambda particles, are extracted from long-range two-particle correlations as functions of particle multiplicity and transverse momentum. For high-multiplicity pp events, a mass ordering is observed for the v[2] values of charged hadrons (mostly pions), K0 short, and Lambda/anti-Lambda, with lighter particle species exhibiting a stronger azimuthal anisotropy signal below pt of about 2 GeV/c. For 13 TeV data, the v[2] signals are also extracted from four- and six-particle correlations for the first time in pp collisions, with comparable magnitude to those from two-particle correlations. These observations are similar to those seen in pPb and PbPb collisions, and support the interpretation of a collective origin for the observed long-range correlations in high-multiplicity pp collisions.},
  archiveprefix = {arxiv},
  keywords = {High Energy Physics - Experiment,Nuclear Experiment},
  file = {/home/matonoliv/snap/zotero-snap/common/Zotero/storage/BTL3UEEX/CMS Collaboration - 2017 - Evidence for collectivity in pp collisions at the .pdf;/home/matonoliv/snap/zotero-snap/common/Zotero/storage/JXXNMHFM/1606.html}
}

@article{cmscollaborationInvestigationEventactivityDependence2020,
  author = {{CMS Collaboration}},
  year = {2020},
  month = nov,
  journal = {J. High Energ. Phys.},
  volume = {2020},
  number = {11},
  eprint = {2007.04277},
  primaryclass = {hep-ex},
  pages = {1},
  issn = {1029-8479},
  doi = {10.1007/JHEP11(2020)001},
  urldate = {2023-04-09},
  abstract = {The ratios of the production cross sections between the excited \$\textbackslash Upsilon\$(2S) and \$\textbackslash Upsilon\$(3S) mesons and the \$\textbackslash Upsilon\$(1S) ground state, detected via their decay into two muons, are studied as a function of the number of charged particles in the event. The data are from proton-proton collisions at \$\textbackslash sqrt\{s\} =\$ 7 TeV, corresponding to an integrated luminosity of 4.8 fb\$\^\{-1\}\$, collected with the CMS detector at the LHC. Evidence of a decrease in these ratios as a function of the particle multiplicity is observed, more pronounced at low transverse momentum \$p\_\textbackslash mathrm\{T\}\^\{\textbackslash mu\textbackslash mu\}\$. For \$\textbackslash Upsilon\$(nS) mesons with \$p\_\textbackslash mathrm\{T\}\^\{\textbackslash mu\textbackslash mu\}\$ \$\textbackslash gt\$ 7 GeV, where most of the data were collected, the correlation with multiplicity is studied as a function of the underlying event transverse sphericity and the number of particles in a cone around the \$\textbackslash Upsilon\$(nS) direction. The ratios are found to be multiplicity independent for jet-like events. The mean \$p\_\textbackslash mathrm\{T\}\^\{\textbackslash mu\textbackslash mu\}\$ values for the \$\textbackslash Upsilon\$(nS) states as a function of particle multiplicity are also measured and found to grow more steeply as their mass increases.},
  archiveprefix = {arxiv},
  keywords = {High Energy Physics - Experiment},
  file = {/home/matonoliv/snap/zotero-snap/common/Zotero/storage/X2H2YEK2/CMS Collaboration - 2020 - Investigation into the event-activity dependence o.pdf;/home/matonoliv/snap/zotero-snap/common/Zotero/storage/IAZCGK2P/2007.html},
  title = {Investigation into the Event-Activity Dependence of {$\Upsilon$}({{nS}}) Relative Production in Proton-Proton Collisions at {$\sqrt{s} = $} 7 {{TeV}}}
}

@article{cmscollaborationMeasurementNuclearModification2019,
  author = {{CMS Collaboration}},
  year = {2019},
  month = mar,
  journal = {Physics Letters B},
  volume = {790},
  eprint = {1805.09215},
  primaryclass = {hep-ex, physics:nucl-ex},
  pages = {270--293},
  issn = {03702693},
  doi = {10.1016/j.physletb.2019.01.006},
  urldate = {2023-04-21},
  abstract = {The cross sections for \$\textbackslash Upsilon\$(1S), \$\textbackslash Upsilon\$(2S), and \$\textbackslash Upsilon\$(3S) production in lead-lead (PbPb) and proton-proton (pp) collisions at \$\textbackslash sqrt\{s\_\{\_\textbackslash mathrm\{NN\}\}\} =\$ 5.02 TeV have been measured using the CMS detector at the LHC. The nuclear modification factors, R\$\_\textbackslash mathrm\{AA\}\$, derived from the PbPb-to-pp ratio of yields for each state, are studied as functions of meson rapidity and transverse momentum, as well as PbPb collision centrality. The yields of all three states are found to be significantly suppressed, and compatible with a sequential ordering of the suppression, R\$\_\textbackslash mathrm\{AA\}\$(\$\textbackslash Upsilon\$(1S)) \${$>\$$} R\$\_\textbackslash mathrm\{AA\}\$(\$\textbackslash Upsilon\$(2S)) \${$>\$$} R\$\_\textbackslash mathrm\{AA\}\$(\$\textbackslash Upsilon\$(3S)) . The suppression of \$\textbackslash Upsilon\$(1S) is larger than that seen at \$\textbackslash sqrt\{s\_\{\_\textbackslash mathrm\{NN\}\}\} =\$ 2.76 TeV, although the two are compatible within uncertainties. The upper limit on the R\$\_\textbackslash mathrm\{AA\}\$ of \$\textbackslash Upsilon\$(3S) integrated over \$p\_\textbackslash mathrm\{T\}\$ and rapidity is 0.094 at 95\% confidence level, which is the strongest suppression observed for any hadron species in heavy ion collisions to date.},
  archiveprefix = {arxiv},
  keywords = {High Energy Physics - Experiment,Nuclear Experiment},
  file = {/home/matonoliv/snap/zotero-snap/common/Zotero/storage/8HAN6T8P/CMS Collaboration - 2019 - Measurement of nuclear modification factors of $U.pdf;/home/matonoliv/snap/zotero-snap/common/Zotero/storage/QLSIWK75/1805.html},
  title = {Measurement of Nuclear Modification Factors of {$\Upsilon$}({{1S}}), {$\Upsilon$}({{2S}}), and {$\Upsilon$}({{3S}}) Mesons in {{PbPb}} Collisions at {$\sqrt{s_{_\mathrm{NN}}} =$} 5.02 {{TeV}}}
}

